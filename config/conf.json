{
  "listeners": {
    "*:8081": {
      "application": "helloPython"
    },
    "*:8700": {
      "application": "example_ruby"
    }
  },
  "applications": {
    "helloPython": {
      "type": "python",
      "processes": 2,
      "path": "/var/www/python",
      "module": "wsgi"
    },
    "example_ruby": {
      "type": "ruby",
      "user": "nobody",
      "processes": 2,
      "script": "/usr/share/doc/unit-ruby/examples/ruby-app.ru"
    }
  }
}
